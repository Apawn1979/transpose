{
	"name": "transpose",
	"displayName": "transpose",
	"description": "Transpose is an extension for VSCode for transposing/swapping selections or characters.",
	"version": "0.0.1",
	"publisher": "v4run",
	"engines": {
		"vscode": "^0.10.6"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.transpose"
	],
	"main": "./out/src/transpose",
	"contributes": {
		"commands": [{
			"command": "extension.transpose",
			"title": "Transpose"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^1.7.5",
		"vscode": "^0.11.0"
	},
    "repository": {
        "type": "git",
        "url": "https://github.com/v4run/transpose.git"
    }
}